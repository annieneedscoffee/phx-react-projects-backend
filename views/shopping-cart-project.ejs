<h1>Shopping Cart Routes</h1>
<h2>Products</h2>
<table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>METHOD</th>
      <th>URL</th>
      <th>EXPECTED REQUEST</th>
      <th>EXPECTED RESPONSE</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>GET (all products)</td>
      <td>/products</td>
      <td></td>
      <td>[{
        "id": 1,
        "name": "Mediocre Iron Watch",
        "priceInCents": 399
    }, ...]
      </td>
    </tr>
    <tr>
      <td>GET (one product)</td>
      <td>/products/1</td>
      <td></td>
      <td>[{
        "id": 1,
        "name": "Mediocre Iron Watch",
        "priceInCents": 399
    }]
      </td>
    </tr>
    <tr>
      <td>POST (one product)</td>
      <td>/products</td>
      <td>{
            "name": "Extra Stable Tight Rope",
            "priceInCents": 1200000
          }
      </td>
      <td>[... , {
            "id":10,
            "name": "Extra Stable Tight Rope",
            "priceInCents": 1200000
          }]
      </td>
    </tr>
    <tr>
      <td>PATCH (one product)</td>
      <td>/products/10</td>
      <td>{
            "name": "Less Dangerous (Than Usual) Tight Rope"
          }
      </td>
      <td>[... , {
            "id":10,
            "name": "Less Dangerous (Than Usual) Tight Rope",
            "priceInCents": 1200000
          }]
      </td>
    </tr>
    <tr>
      <td>Delete (one product)</td>
      <td>/products/1</td>
      <td></td>
      <td>[{
            "id": 2,
            "name": "Heavy Duty Concrete Plate",
            "priceInCents": 499
          }, ...]
      </td>
    </tr>
  </tbody>
</table>
<h2>Items</h2>
<table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>METHOD</th>
      <th>URL</th>
      <th>EXPECTED REQUEST</th>
      <th>EXPECTED RESPONSE</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>GET (all items)</td>
      <td>/items</td>
      <td></td>
      <td>[{
        "id": 1,
        "product_id": 1,
        "quantity": 1
    }, ...]
      </td>
    </tr>
    <tr>
      <td>GET (one item)</td>
      <td>/items/1</td>
      <td></td>
      <td>[{
            "id": 1,
            "product_id": 1,
            "quantity": 1
          }]
      </td>
    </tr>
    <tr>
      <td>POST (one item)</td>
      <td>/items</td>
      <td>{
              "product_id": 2,
              "quantity": 5
          }
      </td>
      <td>[... , {
              "id": 2,
              "product_id": 2,
              "quantity": 5
          }]
      </td>
    </tr>
    <tr>
      <td>PATCH (one item)</td>
      <td>/items/2</td>
      <td>{
              "quantity": 10
          }
      </td>
      <td>[{
              "id": 2,
              "product_id": 2,
              "quantity": 10
          }, ...]
      </td>
    </tr>
    <tr>
      <td>Delete (one item)</td>
      <td>/items/2</td>
      <td></td>
      <td>[{
            "id": 1,
            "product_id": 1,
            "quantity": 1
          }, ...]
      </td>
    </tr>
  </tbody>
</table>
